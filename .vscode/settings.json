{
  "yaml.schemas": {
    "https://taskfile.dev/schema.json": [
      "**/Taskfile.dist.yml",
      "**/Taskfile.yml",
      "**/Taskfile.yaml",
      "**/taskfile.yml",
      "**/taskfile.yaml"
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "third_party": false,
    ".task": false,
    "deps": false,
    "scripts": false,
    ".vscode": false,
    "bun.lock": false,
    "package.json": false,
    "tsconfig.json": false,
    ".gitmodules": false,
    ".gitignore": false,
    "node_modules": false,
    "c_interop/build": false,
    "shaders/spirv": false,
    "bin": false,
    "pkg": false,
    "**/Taskfile.dist.yml": false,
    "tests": false,
    "Appdir": false,
    "*.AppImage": false,
    "dist": false,
    "patches": false,
    "LICENSE.txt": false
  },
  "files.associations": {
    "*.glsl": "glsl",
    "*.scene": "json",
    "socket.h": "c",
    "*.tcc": "cpp",
